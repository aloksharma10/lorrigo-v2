{
  "name": "@lorrigo/utils",
  "version": "0.1.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "clean": "rimraf .turbo && rimraf dist",
    "dev": "tsup --watch",
    "lint": "eslint src/"
  },
  "dependencies": {
    "@lorrigo/db": "workspace:*",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "typescript": "^5.1.6",
    "tsup": "^7.2.0",
    "eslint": "^8.42.0",
    "rimraf": "^5.0.1"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./validations": {
      "import": "./dist/validations/index.js",
      "require": "./dist/validations/index.js",
      "types": "./dist/validations/index.d.ts"
    },
    "./validations/*": {
      "import": "./dist/validations/*.js",
      "require": "./dist/validations/*.js",
      "types": "./dist/validations/*.d.ts"
    },
    "./helpers": {
      "import": "./dist/helpers/index.js",
      "require": "./dist/helpers/index.js",
      "types": "./dist/helpers/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": ["./dist/*"],
      ".": ["./dist/index.d.ts"],
      "validations": ["./dist/validations/index.d.ts"],
      "helpers": ["./dist/helpers/index.d.ts"]
    }
  }
}
